class Solution{
  public:
    int missingNumber(vector<int>& array, int n) {
        // Your code goes here
        sort(array.begin(),array.end());
        int s;
        for(int i=0;i<=n;i++)
    {
        if(array[i]!=i+1)
        {
            s=i+1;
            break;
        }
    }
    return s;
    }
};
